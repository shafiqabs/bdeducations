<!DOCTYPE HTML>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0"/>
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <title>{{ entity.name }}</title>
    {#'assets/module/eventCalendar/css/paragridma.css'
    'assets/module/eventCalendar/css/eventCalendar.css'
    'assets/module/eventCalendar/css/eventCalendar_theme_responsive.css'
    'assets/module/eventCalendar/css/jquery.e-calendar.css'
#}

    {% stylesheets filter='cssrewrite'
    'assets/mobile/slasher/styles/style.css'
    'assets/module/framework/css/framework.css'
    'assets/module/carousel/css/owl.carousel.css'
    'assets/module/carousel/css/owl.theme.css'
    'assets/module/swipebox/css/swipebox.css'
    'assets/module/swipebox/css/colorbox.css'
    'assets/module/jquery-ui/jquery-ui-1.10.1.custom.min.css'
    'assets/module/pageflip/css/bookblock.css'
    'assets/module/pageflip/css/custom.css'
    'assets/module/pageflip/css/jquery.jscrollpane.custom.css'
    'assets/module/accordion/css/accordion.css'
    'assets/mobile/css/custom.css'
    'assets/module/framework/css/icon.css'


%}

    <link rel="stylesheet" href="{{ asset_url }}"/>
{% endstylesheets %}
</head>
<body>

<div id="preloader">
    <div id="status">
        <p class="center-text">
            Loading the content...
            <em>Loading depends on your connection speed!</em>
        </p>
    </div>
</div>
{% block body %}{% endblock %}
{{ include('mobile/slasher/footer.html.twig')}}
{% javascripts
    'assets/module/pageflip/js/modernizr.custom.79639.js'
    'assets/mobile/slasher/scripts/jquery.js'
    'assets/mobile/slasher/scripts/jqueryui.js'
    'assets/module/carousel/js/owl.carousel.min.js'
    'assets/module/swipebox/js/jquery.swipebox.js'
    'assets/module/swipebox/js/colorbox.js'
    'assets/mobile/slasher/scripts/snap.js'
    'assets/mobile/slasher/scripts/contact.js'
    'assets/module/framework/js/framework.js'
    'assets/module/framework/js/framework.launcher.js'
    'assets/module/framework/js/jquery.slicknav.js'
    'assets/module/multi-datepicker/js/jquery.ui.datepicker.js'
    'assets/module/multi-datepicker/js/jquery.ui.core.js'
    'assets/module/multi-datepicker/js/jquery-ui.multidatespicker.js'
    'assets/module/accordion/js/jquery.cookie.js'
    'assets/module/accordion/js/jquery.accordion.js'
    'assets/module/pageflip/js/jquery.mousewheel.js'
    'assets/module/pageflip/js/jquery.jscrollpane.min.js'
    'assets/module/pageflip/js/jquerypp.custom.js'
    'assets/module/pageflip/js/jquery.bookblock.js'
    'assets/module/pageflip/js/page.js'
    'assets/module/pageflip/js/mmenu.js'
    'assets/module/eventCalendar/js/jquery.eventCalendar.js'
    'assets/module/eventCalendar/js/jquery.e-calendar.js'
    'assets/mobile/slasher/scripts/custom.js'

%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<script type="text/javascript">
    {% block javascriptblock %}{% endblock %}
</script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        {% block documentready %}{% endblock %}
        //custom animation for open/close
        $.fn.slideFadeToggle = function(speed, easing, callback) {
            return this.animate({opacity: 'toggle', height: 'toggle'}, speed, easing, callback);
        };
        $('#nav').slicknav();
        $('#nav1').slicknav({
            label: '',
            duration: 1000,
            easingOpen: "easeOutBounce", //available with jQuery UI
            prependTo:'#navigation'
        });
        Page.init();
    });

</script>
</body>
</html>
